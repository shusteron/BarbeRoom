{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [
        {
          "name": "next"
        }
      ],
      "paths": {
        "@/*": ["./BarbeRoom/*"]
      }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/signup/signUpClient.jsx", "app/signup/signUpBarber.jsx", "app/api/users/signup/signUpClient.jsx", "app/api/users/signup/signUpBarber.jsx", "app/api/users/signin/signInClient.jsx", "app/api/users/signin/signInBarber.jsx"],
    "exclude": ["node_modules"]
  }